<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <title>Results</title>
</head>

<body>
    <h1>Results</h1>
    <table>
        <thead>
            <tr>
                <th>Title</th>
                <th>Year</th>
            </tr>
        </thead>
        <tbody>
            <% movies.forEach(function(movie) { %>
                <tr>
                    <td>
                        <%= movie.name %>
                    </td>
                    <td>
                        <%= movie.year %>
                    </td>
                </tr>
                <% }); %>
        </tbody>
    </table>
</body>

</html>